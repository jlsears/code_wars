// return masked string
function maskify(cc) {

var symbol = "#";
var finalDisplay = "";
var lastFour = cc.substring((cc.length-4),cc.length);

if(cc.length < 4) { return lastFour; }

for(i = 0; i < cc.length-4; i++) {

    finalDisplay = finalDisplay + symbol;
  }
    
  return finalDisplay + lastFour;
}

