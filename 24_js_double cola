function whoIsNext(names, r){
  
  var assessing = [];
  
  for(var j = 0; j < names.length; j++) {
    assessing.push(names[j]);
  }
  
  for(var i = 0; i < r; i++) {
    var person = assessing[0];
    assessing.push(person);
    assessing.push(person);
    assessing.shift();
  }
  return person;
}	